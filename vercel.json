{
  "buildCommand": "npm run build",
  "outputDirectory": "web/dist",
  "headers": [
    {
      "source": "/sitemap.xml",
      "headers": [{ "key": "Content-Type", "value": "application/xml" }]
    },
    {
      "source": "/robots.txt",
      "headers": [{ "key": "Content-Type", "value": "text/plain" }]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/|sitemap.xml|robots.txt).*)",
      "destination": "/index.html"
    }
  ]
}
